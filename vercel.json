{
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.ts"
    },
    {
      "src": "/",
      "dest": "/api/index.ts"
    }
  ],
  "functions": {
    "api/index.ts": {
      "runtime": "nodejs18.x",
      "maxDuration": 60
    }
  }
}
